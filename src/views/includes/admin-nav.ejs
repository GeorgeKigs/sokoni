<div class="backdrop"></div>
<header class="main-header">
  <h1>Sokoni</h1>
  <button id="side-menu-toggle">Menu</button>
  <nav class="main-header__nav">
    <ul class="main-header__item-list">
      <%- include('./admin-nav-sections/web/general') %>
      <% if (isAdminLoggedIn&&!isUserLoggedIn) { %>
      <%- include('./admin-nav-sections/web/adminLoggedIn') %>
      <% } %>
    </ul>
    <ul class="main-header__item-list">
      <% if (!isUserLoggedIn&&!isAdminLoggedIn) { %>
      <%- include('./admin-nav-sections/web/guest') %>
      <% } %>
      <% if (isAdminLoggedIn||isUserLoggedIn) { %>
      <%- include('./admin-nav-sections/web/logout') %>
      <% } %>
    </ul>
  </nav>
</header>
<!-- the mobile view navigation setting -->
<nav class="mobile-nav">
  <ul class="mobile-nav__item-list">
    <%- include('./admin-nav-sections/mobile/general') %>
    <% if (isAdminLoggedIn&&!isUserLoggedIn) { %>
    <%- include('./admin-nav-sections/mobile/adminLoggedIn') %>
    <% } %>
    <% if (!isUserLoggedIn&&!isAdminLoggedIn) { %>
    <%- include('./admin-nav-sections/mobile/guest') %>
    <% } %>
    <% if (isAdminLoggedIn||isUserLoggedIn) { %>
    <%- include('./admin-nav-sections/mobile/logout') %>
    <% } %>
  </ul>
</nav>
